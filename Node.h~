#ifndef NODE_H
#define NODE_H

class Node {
 public:
  Node();
  Node* getLeft();
  Node* getRight();
  Node* getData();
  Node* getParent();
  void setLeft(Node*);
  void setRight(Node*);
  void setData(int);
  void setParent(Node*); 
  int getData();
  ~Node();
 private:
  Node* left;
  Node* right;
  int data;
  Node* parent; 
};

#endif
